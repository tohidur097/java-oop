
public class FactDemo{
    
    int fact(int n){
        if(n==1)
            return 1;
        else 
           return n*fact(n-1);
    }
       

    public static void main(String[] args) {
       FactDemo ob=new FactDemo();
      int result= ob.fact(5);
      System.out.println("fact="+result);
    }    
}
