
public class Teacher {
    String name,sex;
    int age;
    
    Teacher(){
        System.out.println();
    }
    
    
    Teacher(String n,String s,int ag){
        name=n;
        sex=s;
        age=ag;
        
    }
    
    void display_info(){
        
    System.out.println("name="+name);  
    System.out.println("sex="+sex);
    System.out.println("age="+age);
     System.out.println();
    }
    
    public static void main(String[] args) {
        
    Teacher teacher1=new Teacher("tohidur rahman","male",25);
     teacher1.display_info();
     
     
    Teacher teacher2=new Teacher("nishat rahman","female",24);
    teacher2.display_info();
    
     Teacher teacher3=new Teacher();
    teacher3.display_info();
        
    
    }
}
